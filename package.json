{
  "name": "City-Of-Denton",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "start": "echo $LD_LIBRARY_PATH & NOLOGIN=false node ./bin/www",
    "dev": "echo $LD_LIBRARY_PATH & NOLOGIN=true node ./bin/www",
    "local:install": "npm i --ignore-scripts",
    "live": "echo $LD_LIBRARY_PATH & nodemon ./bin/www",
    "test": "mocha tests --watch",
    "test:bx": "mocha tests --recursive",
    "postinstall": "unzip libaio.zip && unzip lib.zip && cd libaio && make prefix=$PWD install"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "async": "^2.5.0",
    "body-parser": "~1.15.1",
    "browser-sync": "^2.18.13",
    "cloudant": "^1.8.0",
    "connect-cloudant-store": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "cryptr": "^2.0.0",
    "debug": "~4.1.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "~4.13.4",
    "express-session": "^1.15.6",
    "https": "^1.0.0",
    "jade": "~1.11.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^8.1.0",
    "knex": "^0.21.1",
    "lodash": "^4.17.4",
    "morgan": "~1.10.0",
    "nodemon": "^1.12.1",
    "office-addin-validator": "^1.0.1",
    "office-ui-fabric-js": "^1.3.0",
    "oracle": "^0.4.1",
    "oracledb": "^1.13.1",
    "passport": "^0.4.0",
    "passport-azure-ad": "^3.0.8",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-fuzzy": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "mocha": "^3.5.0",
    "nodemon": "^1.12.1"
  }
}
